{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimalLevel": {
      "Default": "Debug"
    },
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "S3Options": {
    "MinimalLevel": "us-west-2",
    "ServiceUrl": "http://localhost:9000",
    "UseHttp": true,
    "AccessKey": "user",
    "SecretKey": 12345678
  },
  "ConnectionStrings": {
    "DefaultConnection": "host=localhost;port=5433;username=user;password=123;database=EventsAppDb"
  },
  "AllowedHosts": "*"
}
