{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "S3Options": {
    "Region": "us-west-2",
    "ServiceUrl": "http://localhost:9000",
    "UseHttp": true,
    "AccessKey": "user",
    "SecretKey": 12345678,
    "ForcePathStyle": true
  },
  "AuthSettings": {
    "Issuer": "EventsApp.API",
    "Audience": "EventsApp.UI",
    "Secret": "secretsecretsecretsecretsecretsecretsecretsecretsecretsecretsecretsecretsecretsecretsecret",
    "LifeTime": 10
  },
  "ConnectionStrings": {
    "DefaultConnection": "host=localhost;port=5433;username=user;password=123;database=EventsAppDb"
  },
  "AllowedHosts": "*"
}
